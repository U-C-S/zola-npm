{
	"name": "zola-bin",
	"version": "0.4.0-preview.1",
	"author": "U-C-S",
	"license": "MIT",
	"description": "Zola, A Static Site Generator, as a NPM Package",
	"homepage": "https://github.com/U-C-S/zola-tools",
	"repository": {
		"type": "git",
		"url": "https://github.com/U-C-S/zola-tools"
	},
	"type": "module",
	"napi": {
		"name": "zola-bin"
	},
	"main": "index.js",
	"types": "index.d.ts",
	"scripts": {
		"Test_npm_install": "sh ./tests/test_install.sh",
		"Test_CZS": "sh ./tests/test_czs.sh",
		"artifacts": "napi artifacts",
		"build": "node setup.js && napi build --platform --release --cargo-cwd \"zola\"",
		"build:debug": "node setup.js && napi build --platform --cargo-cwd \"zola\"",
		"universal": "napi universal"
	},
	"devDependencies": {
		"@napi-rs/cli": "^2.16.3",
		"@types/node": "^20.5.6",
		"smol-toml": "^1.1.2"
	},
	"engines": {
		"node": ">= 16"
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 2,
		"useTabs": true,
		"printWidth": 110
	},
	"keywords": [
		"zola",
		"tera",
		"static-site-generator",
		"ssg",
		"markdown",
		"rust",
		"html",
		"scss",
		"sass",
		"bin",
		"cli",
		"front-end",
		"framework"
	],
	"optionalDependencies": {
		"zola-bin-win32": "0.17.5-preview.1",
		"zola-bin-darwin": "0.17.5-preview.1",
		"zola-bin-linux": "0.17.5-preview.1"
	}
}
